name: hecke-11a1
on:
  workflow_dispatch:
  push:
    branches: ["**"]
  pull_request:
permissions:
  contents: read
jobs:
  hecke:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - name: Real Hecke/elliptic arithmetic (11a1)
        run: bash scripts/langlands/gate_hecke_11a1.sh
      - name: Show receipts
        run: |
          tail -n +1 analysis/hecke_11a1.tsv | head -n 20
          cat analysis/hecke_11a1_receipt.json
